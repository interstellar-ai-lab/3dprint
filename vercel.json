{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 300
    }
  },
  "env": {
    "PYTHONPATH": "."
  },
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  },
  "includeFiles": [
    "api/static/**"
  ]
} 